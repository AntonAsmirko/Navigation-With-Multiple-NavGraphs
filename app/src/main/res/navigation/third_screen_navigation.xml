<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/third_screen"
    app:startDestination="@id/thirdScreenFragment"
    tools:ignore="UnusedNavigation">
    <fragment
        android:id="@+id/thirdScreenFragment"
        android:name="com.example.navigation.fragments.ThirdScreenFragment"
        android:label="ThirdScreenFragment" >
        <action
            android:id="@+id/action_thirdScreenFragment_self"
            app:destination="@id/thirdScreenFragment"/>
        <argument
            android:name="count"
            app:argType="integer"
            android:defaultValue="0" />
    </fragment>
</navigation>